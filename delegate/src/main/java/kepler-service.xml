<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans        
	http://www.springframework.org/schema/beans/spring-beans.xsd        
	http://code.alibabatech.com/schema/dubbo        
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<import resource="classpath:kepler-server.xml" />
	<bean id="demo" class="com.kepler.demo.start.impl.QuickStartImpl" />
	
	<bean class="com.kepler.service.exported.ExportedDelegate" parent="kepler.service.exproted.abstract"> 
		<constructor-arg index="0" value="com.kepler.demo.start.QuickStart" /> 
		<constructor-arg index="1" ref="demo" />
		<constructor-arg index="2" value="" />
		<constructor-arg index="3" value="0.0.1" />
	</bean> 

</beans>